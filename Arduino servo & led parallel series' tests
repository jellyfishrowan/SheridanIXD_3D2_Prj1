void setup(){
  pinMode(white_LED_1, OUTPUT);
  pinMode(red_LED_1, OUTPUT);
  pinMode(green_LED_1, OUTPUT);
  pinMode(yellow_LED_1, OUTPUT);

  servo.attach(4);
  servo.write(0);
  
//  pinMode(white_LED_2, OUTPUT);
